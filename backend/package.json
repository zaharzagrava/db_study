{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=local nest start",
    "start:dev": "NODE_ENV=local nest start --watch",
    "start:debug": "NODE_ENV=local nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "freshstart:prod": "rm -rf ./dist && yarn run build && cp ./.env ./dist/.env && node --max-old-space-size=8192 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate:create": "npx sequelize-cli migration:generate --name $1",
    "db:migrate:up": "npx sequelize-cli db:migrate",
    "db:migrate:down": "npx sequelize-cli db:migrate:undo",
    "docker": "docker compose -p db_study up",
    "cleanup": "docker volume prune -f && docker network prune -f && docker stop $(docker ps -a -q) || true && docker rm $(docker ps -a -q) || true"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.204.0",
    "@aws-cdk/aws-iam": "^1.204.0",
    "@aws-cdk/aws-lambda": "^1.204.0",
    "@aws-cdk/aws-logs": "^1.204.0",
    "@aws-cdk/aws-rds": "^1.204.0",
    "@aws-cdk/aws-s3": "^1.204.0",
    "@aws-cdk/aws-secretsmanager": "^1.204.0",
    "@aws-cdk/aws-ssm": "^1.204.0",
    "@aws-cdk/core": "^1.204.0",
    "@aws-cdk/custom-resources": "^1.204.0",
    "@faker-js/faker": "^8.3.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^5.1.1",
    "@sequelize/mssql": "^7.0.0-alpha.41",
    "@sequelize/mysql": "^7.0.0-alpha.41",
    "@types/pg": "^8.11.6",
    "axios": "^1.6.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.3.2",
    "joi": "^17.12.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "mariadb": "^3.3.1",
    "mongoose": "^8.1.0",
    "mssql": "^10.0.2",
    "mysql2": "^3.10.1",
    "oracledb": "^6.5.1",
    "pg": "^8.11.3",
    "pg-native": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "tedious": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.4.2",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.3.1",
    "@types/oracledb": "^6.5.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
